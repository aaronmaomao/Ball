<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>ball</display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<listener>
		<listener-class>com.ball.web.biz.WebStartListener</listener-class>
	</listener>
	<servlet>
		<servlet-name>SsqServlet</servlet-name>
		<servlet-class>com.ball.web.biz.SsqServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SsqServlet</servlet-name>
		<url-pattern>/ssq</url-pattern>
	</servlet-mapping>
</web-app>